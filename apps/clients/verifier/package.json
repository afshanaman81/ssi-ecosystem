{
  "name": "@affinidi-interop-test-service/web-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "dev": "REACT_APP_ENVIRONMENT=local PORT=3001 react-scripts start",
    "start:dev": "REACT_APP_ENVIRONMENT=docker PORT=3001 react-scripts start",
    "build": "PUBLIC_URL=\"/interop\" react-scripts build",
    "test": "CI=true react-scripts test --passWithNoTests",
    "eject": "react-scripts eject",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"{,!(node_modules)/**/}*.js\""
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.20.0",
    "bootstrap": "^4.5.2",
    "http-proxy-middleware": "^1.0.6",
    "qrcode.react": "^1.0.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-json-view": "^1.19.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.20.6",
    "prettier": "^2.1.1",
    "dotenv": "^8.2.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "homepage": "/interop",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
